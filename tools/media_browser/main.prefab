<?xml version="1.0"?>
<prefab>
  <eSplitter type="Horizontal" alignx="MinMax" aligny="MinMax" >
    <eElement padding="8,8,8,8" alignx="MinMax" aligny="MinMax" layout_type="Vertical" layout_gap="8" >
      <eEdit name="tag_search_bar" alignx="MinMax" border_color="255,255,255,255" font_size="18" font_color="255,255,255,255" />
      <eElement name="tag_list" alignx="MinMax" aligny="MinMax" layout_type="Vertical" layout_gap="4" />
    </eElement>
    <eElement alignx="MinMax" aligny="MinMax" padding="8,8,8,8" width_factor="4" layout_type="Vertical" layout_gap="16" >
      <eElement alignx="MinMax" layout_type="Horizontal" layout_gap="8" >
        <eEdit name="search_bar" alignx="MinMax" border_color="255,255,255,255" font_size="24" font_color="255,255,255,255" />
        <eButton name="search_btn" font_size="24" text="Search" />
      </eElement>
      <eScroller type="Vertical" alignx="MinMax" aligny="MinMax" >
        <eElement name="list" alignx="MinMax" layout_type="Tile" layout_gap="4" >
        </eElement>
      </eScroller>
    </eElement>
  </eSplitter>
</prefab>
