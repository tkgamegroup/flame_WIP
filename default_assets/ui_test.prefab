<?xml version="1.0"?>
<prefab>
  <eElement alignx="MinMax" aligny="MinMax" layout_type="Vertical">
    <eMenuBar>
      <eMenu text="1" >
        <eMenuItem text="1" />
        <eMenuItem text="2" />
        <eMenuItem text="3" />
        <eMenu text="4" >
          <eMenuItem text="1" />
        </eMenu>
      </eMenu>
      <eMenu text="2" >
        <eMenuItem text="1" />
      </eMenu>
    </eMenuBar>
    <eElement alignx="MinMax" aligny="MinMax" padding="4,4,4,4" layout_type="Horizontal" layout_gap="8">
      <eElement layout_type="Vertical" layout_gap="4">
        <eText text="Hello 123" />
        <eButton text="Button">
          <CcScript>
            entity.find_component("cReceiver").add_mouse_click_listener(function()
            print("Hello World")
            end)
          </CcScript>
        </eButton>
        <eToggle text.text="123" />
        <eEdit width="100" />
        <eDropDown>
          <eDropDownItem text="1" />
          <eDropDownItem text="2" />
          <eDropDownItem text="3" />
        </eDropDown>
        <eDragEdit/>
      </eElement>
      <eElement layout_type="Vertical" layout_gap="4">
        <eScroller name="scroller" type="Vertical" width="200" height="100" padding="4,4,4,4" border="1" auto_width="false" auto_height="false" >
          <eList name="list">
            <eListItem name="item0" text="apple"/>
            <eListItem name="item1" text="banana"/>
            <eListItem name="item2" text="cat"/>
            <eListItem name="item3" text="dog"/>
            <eListItem name="item4" text="egg"/>
            <eListItem name="item5" text="fuck"/>
            <eListItem name="item6" text="girl"/>
            <eListItem name="item7" text="hello"/>
            <eListItem name="item8" text="ice-cream"/>
            <eListItem name="item9" text="java"/>
            <eListItem name="item10" text="king"/>
            <eListItem name="item11" text="little"/>
            <eListItem name="item12" text="monkey"/>
            <eListItem name="item13" text="nigger"/>
          </eList>
        </eScroller>
        <eTree width="200" height="200" auto_width="false" auto_height="false" padding="8,8,8,8" border="1.0">
          <eTreeNode title="A" >
            <eTreeLeaf title="B" />
          </eTreeNode >
          <eTreeNode title="C" />
        </eTree>
        <eSplitter type="Horizontal">
          <entity>
            <cElement width="10" height="10" alignx="MinMax" aligny="MinMax" fill_color="255,0,0,255" />
          </entity>
          <entity>
            <cElement width="10" height="10" alignx="MinMax" aligny="MinMax" fill_color="0,255,0,255" />
          </entity>
        </eSplitter>
      </eElement>
      <eWindow fill_color="240,240,240,50" pos="400,30">
        <cBlur radius="10" />
      </eWindow>
    </eElement>
  </eElement>
</prefab>
